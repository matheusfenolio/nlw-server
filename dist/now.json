{
    "version": 2,
    "builds": [
        { "src": "src/server.js", "use": "@now/node-server" },
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "routes": [
    {
        "src": "/",
        "dest": "src/server.js"
    },
    {
        "src": "/items",
        "dest": "src/server.js"
    },
    {
        "src": "/points",
        "dest": "src/server.js"
    },
    {
        "src": "/uploads",
        "dest": "src/server.js"
    }]

}
